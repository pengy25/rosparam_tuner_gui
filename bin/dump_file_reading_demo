#! /usr/bin/env python
import rospy
from rosparam_tuner_gui import readDumpFile

def main():
  rospy.init_node("dump_file_reader_demo")
  while rospy.Time.now().to_sec() == 0:
    pass

  argv = rospy.myargv()
  if len(argv) < 2:
    print "Usage: rosrun rosparam_tuner_gui dump_file_reader_demo FILE.yaml"
    return

  filename = argv[1]
  params = readDumpFile(filename)
  print params

if __name__ == "__main__":
  main()
